        "def fact(num):\n",
        "  ans= 1\n",
        "  if num!= 0:\n",
        "    for i in range(1, num+1):\n",
        "      ans= ans * i\n",
        "  else:\n",
        "    ans = 0 \n",
        "  return ans\n",
        "\n",
        "num= int(input(\"Enter a number\"))\n",
        "print(fact(num))"
