        "  return num // den\n",
        "\n",
        "def mod_div(func):\n",
        "  def mod(a , b):\n",
        "    if a < b:\n",
        "      a, b = b, a\n",
        "    return func(a, b)\n",
        "    \n",
        "  return mod \n",
        "\n",
        "a, b = [int(i) for i in input(\"Enter the numerator and denominator:- \").split()]\n",
        "m = mod_div(div)\n",
